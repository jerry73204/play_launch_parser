<?xml version="1.0" encoding="UTF-8"?>
<!-- Minimal test: XML pushes namespace, includes Python that returns XML include -->
<launch>
  <arg name="fixture_dir" default="$(dirname)"/>

  <!-- Push namespace at XML level -->
  <push-ros-namespace namespace="/test_ns"/>

  <!-- Include Python file -->
  <include file="$(dirname)/debug_ns_python.launch.py">
    <arg name="fixture_dir" value="$(var fixture_dir)"/>
  </include>

  <pop-ros-namespace/>
</launch>
